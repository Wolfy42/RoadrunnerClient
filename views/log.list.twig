{% extends "layout.twig" %}

{% block content %}

	<h2>List of logs</h2>

	<table>
		<thead>
			<tr>
				<th>Timestamp</th>
				<th>Type</th>
				<th>Value</th>
			</tr>
		</thead>
		<tbody>
			{% for log in log_list %}
				<tr>
					<td>{{log.value.timestamp|date("Y-m-d H:i:s") }}</td>
					<td>{{log.value.logType}}</td>
					<td>{{log.value.value}}</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

{% endblock %}